Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"SpatialCompositionToRnum", "[", 
    RowBox[{"rules_List", ",", 
     RowBox[{"ntimesteps_Integer", ":", "1"}], ",", 
     RowBox[{"k_Integer", ":", "2"}]}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"nrules", "=", 
        RowBox[{"Length", "[", "rules", "]"}]}], ",", 
       RowBox[{"rnumradius", "=", 
