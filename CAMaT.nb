Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"FindCACycles", "[", 
    RowBox[{"ruleform_", ",", "ic_List", ",", 
     RowBox[{"initIter_Integer", ":", "0"}], ",", "finalIter_Integer", ",", 
     RowBox[{"shiftOpt_Symbol", ":", "noshift"}]}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"finalIter", "<", "initIter"}], ",", 
