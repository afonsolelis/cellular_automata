Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"OBS", ":", " ", 
    RowBox[{
     RowBox[{"OutputConjugateRule", "@", "NeighbourhoodConjugateRule"}], " ", 
