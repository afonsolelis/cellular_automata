              RowBox[{"{", 
               RowBox[{"Length", "[", "aux", "]"}], "}"}]}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"ans", "\[LeftDoubleBracket]", 
               RowBox[{"i", ",", 
                RowBox[{
                "activenhspos", "\[LeftDoubleBracket]", "j", 
                 "\[RightDoubleBracket]"}]}], "\[RightDoubleBracket]"}], "=", 
              RowBox[{"aux", "\[LeftDoubleBracket]", 
               RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}]}], ",", 
