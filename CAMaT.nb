             RowBox[{"First", "[", "#", "]"}], ",", 
             RowBox[{"Last", "[", "#", "]"}]}], "}"}], "&"}], "/@", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"CASpatialComposition", "[", 
              RowBox[{"rules", ",", "#", ",", "ntimesteps"}], "]"}], "&"}], "/@", 
            RowBox[{"Tuples", "[", 
             RowBox[{
              RowBox[{"Range", "[", 
