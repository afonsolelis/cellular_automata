Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"RuleSpaceSize", "[", 
    RowBox[{
     RowBox[{"k_Integer", ":", "2"}], ",", 
     RowBox[{"r_", ":", "1"}]}], "]"}], ":=", " ", 
   SuperscriptBox["k", 
    SuperscriptBox["k", 
     RowBox[{"\[LeftFloor]", 
      RowBox[{
       RowBox[{"2", "r"}], "+", "1"}], "\[RightFloor]"}]]]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
