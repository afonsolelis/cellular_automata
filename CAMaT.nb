            RowBox[{
             RowBox[{
              RowBox[{"TemplateToAlternatives", "[", 
               RowBox[{"#", ",", "k"}], "]"}], "&"}], "/@", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{"RuleTemplateVars", "[", 
                 RowBox[{"{", "#", "}"}], "]"}], "&"}], "/@", "template"}], 
              ")"}]}]}], "}"}]}], "]"}]}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"<<", "Algebra`InequalitySolve`"}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"ValidSubstitutionsForTemplate", "[", 
       RowBox[{
        RowBox[{"k_Integer", ":", "2"}], ",", 
