 StyleBox["], MagF[ic",
  FontFamily->"Times New Roman"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["", "t"], TraditionalForm]],
  FontFamily->"Times New Roman",ExpressionUUID->
  "4142e941-a759-4666-957b-d7326ac8ea79"],
 StyleBox["], {ic",
  FontFamily->"Times New Roman"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["", "t"], TraditionalForm]],
  FontFamily->"Times New Roman",ExpressionUUID->
  "23132726-8a60-455f-bf19-60cb9d5737d3"],
 StyleBox[", ic",
