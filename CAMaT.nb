Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ExceptionPatterns", "[", 
    RowBox[{
     RowBox[{"k_Integer", ":", "2"}], ",", 
     RowBox[{"r_", ":", "1"}], ",", "intemplate_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Union", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
