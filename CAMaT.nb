Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DNF2AlgebraicForm", "[", "dnfForm_", "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"MemberQ", "[", 
