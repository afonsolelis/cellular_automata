                RowBox[{"k", "-", "1"}]}], "]"}]}], ")"}]}], "/.", 
           RowBox[{"{", 
            RowBox[{"temp", "\[Rule]", "#"}], "}"}]}], "&"}], "/@", 
         "template"}], ",", "\[IndentingNewLine]", "_", ",", 
        RowBox[{"MapThread", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"List", "[", 
            RowBox[{"#1", ",", 
             RowBox[{"Sequence", "@@", "#2"}]}], "]"}], "&"}], ",", 
          RowBox[{"{", 
