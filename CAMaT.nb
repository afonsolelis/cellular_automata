          RowBox[{"2", "r"}], "\[RightCeiling]"}], "+", "1"}]]}], "]"}], ",", 
     "k", ",", "r"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"RuleTablePartitioned", "[", 
    RowBox[{"rnum_Integer", ",", 
     RowBox[{"k_Integer", ":", "2"}], ",", 
     RowBox[{"r_", ":", "1"}]}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
         "&"}], "/@", "#"}], ",", 
       RowBox[{"#", "\[LeftDoubleBracket]", 
        RowBox[{"1", ",", "2"}], "\[RightDoubleBracket]"}]}], "}"}], "&"}], "/@", 
    RowBox[{"Reverse", "[", 
     RowBox[{"SortBy", "[", 
      RowBox[{
