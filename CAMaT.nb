             "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
             "&"}], "/@", "#"}], ")"}], "&"}], "/@", "#"}], ")"}], "&"}], "/@", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"Partition", "[", 
          RowBox[{"#", ",", 
           RowBox[{
            RowBox[{"Length", "[", "#", "]"}], "/", "2"}]}], "]"}], "&"}], 
        "[", 
        RowBox[{"GatherBy", "[", 
         RowBox[{
          RowBox[{"RuleTable", "[", 
           RowBox[{"rnum", ",", "k", ",", "r"}], "]"}], ",", 
          RowBox[{
           RowBox[{"Count", "[", 
