 RowBox[{
  RowBox[{
   RowBox[{"ReversibleRuleQ", "[", 
    RowBox[{"rnum_Integer", ",", 
     RowBox[{"k_Integer", ":", "2"}], ",", " ", 
     RowBox[{"r_", ":", "1"}]}], "]"}], ":=", "\[IndentingNewLine]", 
