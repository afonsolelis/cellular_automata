Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"DecomposeCA", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"rnum_Integer", ",", "k_Integer", ",", 
        RowBox[{"r_", ":", "1"}]}], "}"}], ",", 
