Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"TotalisticQ", "[", 
    RowBox[{"rnum_Integer", ",", 
     RowBox[{"k_Integer", ":", "2"}], ",", 
     RowBox[{"r_", ":", "1"}]}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"{", "1", "}"}], "===", 
      RowBox[{"Union", "[", 
       RowBox[{"Length", "/@", 
        RowBox[{"(", 
         RowBox[{"Union", "/@", 
          RowBox[{"GatherBy", "[", 
           RowBox[{
            RowBox[{
