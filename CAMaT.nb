     RowBox[{"k_Integer", ":", "2"}], ",", 
     RowBox[{"r_", ":", "1"}]}], "]"}], ":=", 
   RowBox[{"Tuples", "[", 
    RowBox[{
     RowBox[{"Range", "[", 
      RowBox[{
