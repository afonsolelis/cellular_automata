Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"TemplateToAlternatives", "[", 
      RowBox[{"template_", ",", 
       RowBox[{"k_Integer", ":", "2"}], ",", 
