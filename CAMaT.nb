          RowBox[{"rnum", ",", "k", ",", "r"}], "]"}]}], "]"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"Flatten", "[", 
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"#", "/.", "\[InvisibleSpace]", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"a_", "\[Rule]", "s_"}], ")"}], "\[RuleDelayed]", 
              RowBox[{
               RowBox[{"rtab", "\[LeftDoubleBracket]", 
                RowBox[{
                 RowBox[{"i", " ", "k"}], "+", "a", "+", "1"}], 
                "\[RightDoubleBracket]"}], "\[Rule]", 
               RowBox[{
                RowBox[{
                 SuperscriptBox["k", 
