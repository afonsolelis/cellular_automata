       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"templatepartswithvariables", "=", 
         RowBox[{"Union", "[", 
          RowBox[{"Select", "[", 
           RowBox[{"intemplate", ",", 
            RowBox[{
             RowBox[{"\[Not]", 
              RowBox[{"MemberQ", "[", 
               RowBox[{
                RowBox[{"Append", "[", 
