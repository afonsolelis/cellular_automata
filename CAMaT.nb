  0, 0, 1],ExpressionUUID->"69d0acd2-b998-40d1-8882-b04f50d2624c"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"NearlyBalancedRBS", "[", "len_Integer", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"rnd", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"EvenQ", "[", "len", "]"}], ",", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", "1"}], ",", "1"}], "}"}], "\[LeftDoubleBracket]", 
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "2"}], "}"}], "]"}], "\[RightDoubleBracket]"}], 
         ",", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"0", ",", "1"}], "}"}], "\[LeftDoubleBracket]", 
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "2"}], "}"}], "]"}], 
          "\[RightDoubleBracket]"}]}], "]"}]}], "}"}], ",", 
     RowBox[{"MapAt", "[", 
      RowBox[{
       RowBox[{"1", "&"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", "len", "}"}]}], "]"}], ",", 
       RowBox[{"List", "/@", 
        RowBox[{"RandomSample", "[", 
         RowBox[{
          RowBox[{"Range", "[", "len", "]"}], ",", 
          RowBox[{
           RowBox[{"\[LeftFloor]", 
            FractionBox["len", "2"], "\[RightFloor]"}], "+", "rnd"}]}], 
         "]"}]}]}], "]"}]}], "]"}]}], ";"}]], "Input",
